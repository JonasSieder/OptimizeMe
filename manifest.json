{
    "name": "OptimizeMe",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
        "default_title": "OptimizeMe",
        "default_popup": "src/index.html",
        "default_icon": "icons/icon-16.png"
    },
    "permissions": [
      "storage",
      "notifications",
      "nativeMessaging",
      "windows"
    ],
    "background": {
      "service_worker": "background.js"
    }
}